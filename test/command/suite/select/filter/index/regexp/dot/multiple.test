table_create Memos TABLE_NO_KEY
column_create Memos content COLUMN_SCALAR Text

table_create RegexpTokens TABLE_PAT_KEY ShortText \
  --default_tokenizer TokenRegexp
column_create RegexpTokens memos_content COLUMN_INDEX|WITH_POSITION \
  Memos content

load --table Memos
[
{"content": "alphabet: abcXYdeXYZfXg"},
{"content": "space:    abc  de   f g"},
{"content": "not match: abcXYde   fg"}
]

#@add-important-log-levels info
log_level --level info
#@collect-query-log true
select Memos \
  --filter 'content @~ "abc..de...f.g"' \
  --output_columns 'content, _score'
#@collect-query-log false
log_level --level notice
#@remove-important-log-levels info
