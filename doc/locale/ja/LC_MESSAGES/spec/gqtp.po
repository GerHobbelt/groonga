msgstr ""

msgid "Protocol type."
msgstr "プロトコルの種類。"

msgid "``query_type``"
msgstr ""

msgid "Content type of body."
msgstr "ボディのコンテントタイプ。"

msgid "``key_length``"
msgstr ""

msgid "2byte"
msgstr ""

msgid "Not used."
msgstr "未使用。"

msgid "``level``"
msgstr ""

msgid "``flags``"
msgstr ""

msgid "Flags."
msgstr "フラグ。"

msgid "``status``"
msgstr ""

msgid "Return code."
msgstr "リターンコード。"

